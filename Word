#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a=0,b=0;
   string s;
   cin>>s;
   for(int i=0;i<s.size();i++)
   {
       if(isupper(s[i]))
        a++;
       else
        b++;
   }
   if(a>b)
   {
       for(int i=0;i<s.size();i++)
       {
            s[i]=toupper(s[i]);
       }
       cout<<s;
   }
   else
   {
       for(int i=0;i<s.size();i++)
       {
            s[i]=tolower(s[i]);
       }
       cout<<s;
   }

    return 0;
}
